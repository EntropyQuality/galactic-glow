    "tokenColors": [
        {"scope": ["keyword"], "settings": {"foreground": "#4A90E2"}},
        {"scope": ["constant.numeric"], "settings": {"foreground": "#FF3B30"}},
        {"scope": ["entity.name.class", "entity.name.type.class"], "settings": {"fontStyle": "italic", "foreground": "#4A90E2"}},
        {"scope": ["entity.name.function"], "settings": {"foreground": "#FFCC00"}},
        {"scope": ["variable", "variable.parameter", "variable.other"], "settings": {"foreground": "#D9D9D9"}},
        {"scope": ["keyword.operator", "storage.modifier"], "settings": {"foreground": "#4A90E2"}},
        {"scope": ["comment"], "settings": {"fontStyle": "italic", "foreground": "#6D7891"}},
        {"scope": ["string"], "settings": {"foreground": "#FF3B30"}},
        {"scope": ["string.quoted.double.json"], "settings": {"foreground": "#FFCC00"}},
        {"scope": ["punctuation"], "settings": {"foreground": "#5AA2F0"}},
        {"scope": ["invalid", "invalid.illegal"], "settings": {"foreground": "#FF6B68"}},
        {"scope": ["entity.name.tag"], "settings": {"foreground": "#4A90E2"}},
        {"scope": ["storage.type"], "settings": {"foreground": "#4A90E2"}},
        {"scope": ["meta.tag.attributes"], "settings": {"foreground": "#D9D9D9"}},
        {"scope": ["entity.name.section.markdown"], "settings": {"foreground": "#FFCC00"}},
        {"scope": ["heading.markdown"], "settings": {"foreground": "#4A90E2"}},
        {"scope": ["markup.bold.markdown", "markup.bold"], "settings": {"fontStyle": "bold", "foreground": "#FF3B30"}},
        {"scope": ["markup.italic.markdown", "markup.italic"], "settings": {"fontStyle": "italic", "foreground": "#FF3B30"}},
        {"scope": ["variable.language"], "settings": {"foreground": "#E16464"}},
        {"scope": ["meta.property-value"], "settings": {"foreground": "#FFD700"}},
        {"scope": ["entity.other.attribute-name.pseudo-class"], "settings": {"foreground": "#FF69B4"}},
        {"scope": ["meta.function-call.generic"], "settings": {"fontStyle": "bold", "foreground": "#FFD700"}},
        {"scope": ["entity.other.inherited-class"], "settings": {"foreground": "#AEFFAE"}},
        {"scope": ["constant.language"], "settings": {"foreground": "#BE5046"}},
        {"scope": ["support.function"], "settings": {"foreground": "#FFCC00"}},
        {"scope": ["support.type"], "settings": {"foreground": "#98C379"}},
        {"scope": ["support.type.property-name"], "settings": {"foreground": "#6D7891"}},
        {"scope": ["support.type.property-name.json"], "settings": {"foreground": "#FF3B30"}}
    ],
    "semanticHighlighting": true
  }
  