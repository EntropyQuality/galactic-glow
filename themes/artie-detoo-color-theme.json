    "tokenColors": [
        {"scope": ["keyword"], "settings": {"foreground": "#E57878"}},
        {"scope": ["constant.numeric"], "settings": {"foreground": "#FFD76D"}},
        {"scope": ["entity.name.class", "entity.name.type.class"], "settings": {"fontStyle": "italic", "foreground": "#4EB4FA"}},
        {"scope": ["entity.name.function"], "settings": {"foreground": "#A4CD68"}},
        {"scope": ["variable", "variable.parameter", "variable.other"], "settings": {"foreground": "#D0D7DE"}},
        {"scope": ["keyword.operator", "storage.modifier"], "settings": {"foreground": "#E57878"}},
        {"scope": ["comment"], "settings": {"fontStyle": "italic", "foreground": "#626D76"}},
        {"scope": ["string"], "settings": {"foreground": "#A4CD68"}},
        {"scope": ["string.quoted.double.json"], "settings": {"foreground": "#4EDFDD"}},
        {"scope": ["punctuation"], "settings": {"foreground": "#D0D7DE"}},
        {"scope": ["invalid", "invalid.illegal"], "settings": {"foreground": "#E57878"}},
        {"scope": ["entity.name.tag"], "settings": {"foreground": "#E57878"}},
        {"scope": ["storage.type"], "settings": {"foreground": "#4EB4FA"}},
        {"scope": ["meta.tag.attributes"], "settings": {"foreground": "#D0D7DE"}},
        {"scope": ["entity.name.section.markdown"], "settings": {"foreground": "#A4CD68"}},
        {"scope": ["heading.markdown"], "settings": {"foreground": "#E57878"}},
        {"scope": ["markup.bold.markdown", "markup.bold"], "settings": {"fontStyle": "bold", "foreground": "#FFD76D"}},
        {"scope": ["markup.italic.markdown", "markup.italic"], "settings": {"fontStyle": "italic", "foreground": "#E57878"}},
        {"scope": ["variable.language"], "settings": {"foreground": "#4EB4FA"}},
        {"scope": ["meta.property-value"], "settings": {"foreground": "#D0D7DE"}},
        {"scope": ["entity.other.attribute-name.pseudo-class"], "settings": {"foreground": "#4EB4FA"}},
        {"scope": ["meta.function-call.generic"], "settings": {"fontStyle": "bold", "foreground": "#D0D7DE"}},
        {"scope": ["entity.other.inherited-class"], "settings": {"foreground": "#A4CD68"}},
        {"scope": ["constant.language"], "settings": {"foreground": "#E57878"}},
        {"scope": ["support.function"], "settings": {"foreground": "#4EDFDD"}},
        {"scope": ["support.type"], "settings": {"foreground": "#4EB4FA"}},
        {"scope": ["support.type.property-name"], "settings": {"foreground": "#626D76"}},
        {"scope": ["support.type.property-name.json"], "settings": {"foreground": "#4EB4FA"}}
    ],
    "semanticHighlighting": true
}
