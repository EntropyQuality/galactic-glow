    "tokenColors": [
        {"scope": ["keyword"], "settings": {"foreground": "#ff00ff"}},
        {"scope": ["constant.numeric"], "settings": {"foreground": "#ff77ff"}},
        {"scope": ["entity.name.class", "entity.name.type", "entity.name.enum"], "settings": {"fontStyle": "italic", "foreground": "#ff77ff"}},
        {"scope": ["entity.name.function"], "settings": {"foreground": "#db6ad1"}},
        {"scope": ["variable", "variable.parameter", "variable.other"], "settings": {"foreground": "#d9abdb"}},
        {"scope": ["keyword.operator", "storage.modifier"], "settings": {"foreground": "#ffe600"}},
        {"scope": ["comment"], "settings": {"fontStyle": "italic", "foreground": "#816b87"}},
        {"scope": ["string"], "settings": {"foreground": "#c56ed4"}},
        {
            "scope": ["string.quoted.double.json"],
            "settings": {"foreground": "#ffccff"}
        },
        {"scope": ["storage.type", "support.class"], "settings": {"foreground": "#3fa7a7"}},
        {"scope": ["support.function"], "settings": {"foreground": "#42cfcf"}},
        {"scope": ["variable.annotation"], "settings": {"foreground": "#31a2a2"}},
        {"scope": ["invalid"], "settings": {"foreground": "#FF0000"}},
        {"scope": ["entity.name.tag"], "settings": {"foreground": "#b084b6"}},
        {"scope": ["punctuation.definition.tag"], "settings": {"foreground": "#42cfcf"}},
        {"scope": ["meta.tag"], "settings": {"foreground": "#db6ad1"}},
        {"scope": ["support.constant"], "settings": {"foreground": "#3fa7a7"}},
        {"scope": ["entity.other.attribute-name"], "settings": {"foreground": "#31a2a2"}},
        {"scope": ["text.html.basic"], "settings": {"foreground": "#e1bbdb"}},
        {"scope": ["entity.name.tag.html"], "settings": {"foreground": "#b084b6"}},
        
        {"scope": ["entity.other.attribute-name.html"], "settings": {"foreground": "#31a2a2"}},
        
        {"scope": ["punctuation.separator.key-value.html"], "settings": {"foreground": "#42cfcf"}},
        
        {"scope": ["punctuation.definition.tag.html"], "settings": {"foreground": "#42cfcf"}},
        
        {"scope": ["meta.tag.metadata"], "settings": {"foreground": "#db6ad1"}},
        
        {"scope": ["string.quoted.double.html"], "settings": {"foreground": "#ffccff"}},
        
        {"scope": ["comment.block.documentation.html"], "settings": {"foreground": "#816b87"}},
        
        {"scope": ["constant.character.entity.html"], "settings": {"foreground": "#3fa7a7"}},
        {
            "scope": ["entity.name.tag.html"],
            "settings": {
                "foreground": "#FFDD42"
            }
        },
        {
            "scope": ["entity.name.tag.css"],
            "settings": {
                "foreground": "#FFDD42"
            }
        },
        {
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#6FC1A5" 
            }
        },
        {
            "scope": ["entity.name.section.markdown"],
            "settings": {
                "foreground": "#9FA8DA" 
            }
        },
        {
            "scope": ["markup.bold.markdown", "markup.bold"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFAB40" 
            }
        },
        {"scope": ["variable.language"], "settings": {"foreground": "#ff77ff"}},
        {"scope": ["constant.language"], "settings": {"foreground": "#ADD8E6"}},
        {"scope": ["support.type"], "settings": {"foreground": "#F4BF75"}},
        {"scope": ["support.type.property-name.json"], "settings": {"foreground": "#b084b6"}},
    ]
}
