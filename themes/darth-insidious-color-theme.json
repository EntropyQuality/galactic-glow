    "tokenColors": [
        {"scope": ["keyword"], "settings": {"foreground": "#991414"}},
        {"scope": ["constant.numeric"], "settings": {"foreground": "#007ACC"}},
        {"scope": ["entity.name.class", "entity.name.type.class"], "settings": {"fontStyle": "italic", "foreground": "#A767A1"}},
        {"scope": ["entity.name.function"], "settings": {"foreground": "#FFC74A"}},
        {"scope": ["variable", "variable.parameter", "variable.other"], "settings": {"foreground": "#A8A8A8"}},
        {"scope": ["keyword.operator", "storage.modifier"], "settings": {"foreground": "#991414"}},
        {"scope": ["comment"], "settings": {"fontStyle": "italic", "foreground": "#6A6A6A"}},
        {"scope": ["string"], "settings": {"foreground": "#007ACC"}},
        {"scope": ["string.quoted.double.json"], "settings": {"foreground": "#FFC74A"}},
        {"scope": ["punctuation"], "settings": {"foreground": "#FF8C00"}},
        {"scope": ["invalid", "invalid.illegal"], "settings": {"foreground": "#FF6B68"}},
        {"scope": ["entity.name.tag"], "settings": {"foreground": "#6D0E0E"}},
        {"scope": ["storage.type"], "settings": {"foreground": "#A767A1"}},
        {"scope": ["meta.tag.attributes"], "settings": {"foreground": "#A8A8A8"}},
        {"scope": ["entity.name.section.markdown"], "settings": {"foreground": "#FFC74A"}},
        {"scope": ["heading.markdown"], "settings": {"foreground": "#991414"}},
        {"scope": ["markup.bold.markdown", "markup.bold"], "settings": {"fontStyle": "bold", "foreground": "#007ACC"}},
        {"scope": ["markup.italic.markdown", "markup.italic"], "settings": {"fontStyle": "italic", "foreground": "#007ACC"}},
        {"scope": ["variable.language"], "settings": {"foreground": "#E16464"}},
        {"scope": ["meta.property-value"], "settings": {"foreground": "#FFD700"}},
        {"scope": ["entity.other.attribute-name.pseudo-class"], "settings": {"foreground": "#FF69B4"}},
        {"scope": ["meta.function-call.generic"], "settings": {"fontStyle": "bold", "foreground": "#FFD700"}},
        {"scope": ["entity.other.inherited-class"], "settings": {"foreground": "#AEFFAE"}},
        {"scope": ["constant.language"], "settings": {"foreground": "#BE5046"}},
        {"scope": ["support.function"], "settings": {"foreground": "#FFC74A"}},
        {"scope": ["support.type"], "settings": {"foreground": "#98C379"}},
        {"scope": ["support.type.property-name"], "settings": {"foreground": "#5F5F5F"}},
        {"scope": ["support.type.property-name.json"], "settings": {"foreground": "#A767A1"}}
    ],
    "semanticHighlighting": true
}
       
