{
    "name": "Luke Skywriter",
    "type": "light",
    "colors": {
        "editor.background": "#1E1E3F",
        "editor.foreground": "#DADCEC",
        "editor.lineHighlightBackground": "#2A2A4A",
        "editorCursor.foreground": "#FFE400",
        "sideBar.background": "#2A2A40",
        "sideBar.foreground": "#B9BAE1",
        "sideBar.border": "#D1CF00",
        "sideBarTitle.foreground": "#FFE400",
        "sideBarSectionHeader.background": "#343455",
        "sideBarSectionHeader.foreground": "#DADCEC",
        "activityBar.background": "#2A2A40",
        "activityBar.foreground": "#D1CF00",
        "activityBar.inactiveForeground": "#797993",
        "activityBarBadge.background": "#D1CF00",
        "activityBarBadge.foreground": "#1E1E3F",
        "statusBar.background": "#343455",
        "statusBar.foreground": "#B9BAE1",
        "statusBar.border": "#2A2A40",
        "statusBar.noFolderBackground": "#343455",
        "statusBar.debuggingBackground": "#4A4A5F",
        "statusBarItem.prominentBackground": "#3A3A50",
        "statusBarItem.hoverBackground": "#4A4A5F",
        "statusBarItem.prominentForeground": "#B9BAE1",
        "scrollbar.shadow": "#2A2A40",
        "scrollbarSlider.background": "#4A4A5F",
        "scrollbarSlider.activeBackground": "#FFE400",
        "scrollbarSlider.hoverBackground": "#797993",
        "peekView.border": "#D1CF00",
        "peekViewEditor.background": "#1E1E3F",
        "peekViewResult.background": "#343455",
        "editorGroup.border": "#3A3A50",
        "editorGroupHeader.tabsBackground": "#343455",
        "editorGroup.dropBackground": "#797993",
        "tab.activeBackground": "#343455",
        "tab.activeForeground": "#DADCEC",
        "tab.inactiveBackground": "#2A2A40",
        "tab.inactiveForeground": "#797993",
        "tab.hoverBackground": "#3A3A50",
        "tab.unfocusedActiveForeground": "#797993",
        "tab.unfocusedInactiveForeground": "#68687B",
        "tab.border": "#3A3A50",
        "tab.lastPinnedBorder": "#FFE400",
        "tab.unfocusedHoverForeground": "#797993",
        "input.background": "#343455",
        "input.foreground": "#B9BAE1",
        "input.border": "#3A3A50",
        "button.background": "#FFE400",
        "button.hoverBackground": "#D1CF00",
        "button.foreground": "#1E1E3F",
        "dropdown.background": "#343455",
        "dropdown.foreground": "#B9BAE1",
        "dropdown.border": "#3A3A50",
        "menu.background": "#343455",
        "menu.foreground": "#B9BAE1",
        "menu.selectionBackground": "#716700",
        "menu.selectionForeground": "#DADCEC",
        "menu.border": "#3A3A50",
        "terminal.foreground": "#B9BAE1",
        "terminal.background": "#1E1E3F",
        "terminal.ansiBlack": "#1E1E3F",
        "terminal.ansiRed": "#FFE400",
        "terminal.ansiGreen": "#6FCF97",
        "terminal.ansiYellow": "#B9BAE1",
        "terminal.ansiBlue": "#343455",
        "terminal.ansiMagenta": "#D1CF00",
        "terminal.ansiCyan": "#6FCF97",
        "terminal.ansiWhite": "#B9BAE1",
        "terminalCursor.foreground": "#D1CF00",
        "focusBorder": "#D1CF00",
        "badge.background": "#D1CF00",
        "badge.foreground": "#1E1E3F",
        "list.activeSelectionBackground": "#3A3A50",
        "list.activeSelectionForeground": "#DADCEC",
        "list.hoverBackground": "#3A3A50",
        "list.hoverForeground": "#B9BAE1",
        "list.highlightForeground": "#FFE400",
        "list.inactiveSelectionBackground": "#343455",
        "list.inactiveSelectionForeground": "#B9BAE1",
        "titleBar.activeBackground": "#343455",
        "titleBar.activeForeground": "#B9BAE1",
        "titleBar.inactiveBackground": "#2A2A40",
        "titleBar.inactiveForeground": "#797993",
        "titleBar.border": "#3A3A50",
        "statusBarItem.activeBackground": "#343455",
        "pickerGroup.foreground": "#DADCEC",
        "pickerGroup.border": "#3A3A50",
        "walkThrough.embeddedEditorBackground": "#1E1E3F",
        "notifications.background": "#2E2E4F"
    },
    "tokenColors": [
        {"scope": ["keyword"], "settings": {"foreground": "#FFE400"}},
        {"scope": ["constant.numeric"], "settings": {"foreground": "#6FCF97"}},
        {"scope": ["entity.name.class", "entity.name.type.class"], "settings": {"fontStyle": "italic", "foreground": "#D1CF00"}},
        {"scope": ["entity.name.function"], "settings": {"foreground": "#6FCF97"}},
        {"scope": ["variable", "variable.parameter", "variable.other"], "settings": {"foreground": "#B9BAE1"}},
        {"scope": ["keyword.operator", "storage.modifier"], "settings": {"foreground": "#FFE400"}},
        {"scope": ["comment"], "settings": {"fontStyle": "italic", "foreground": "#68687B"}},
        {"scope": ["string"], "settings": {"foreground": "#83ADB5"}},
        {"scope": ["support.type.property-name.json"], "settings": {"foreground": "#B9BAE1"}},
        {"scope": ["string.quoted.double.json"], "settings": {"foreground": "#6FCF97"}},
        {"scope": ["punctuation"], "settings": {"foreground": "#E67E22"}},
        {"scope": ["invalid", "invalid.illegal"], "settings": {"foreground": "#FFFFFF"}},
        {"scope": ["invalid.deprecated"], "settings": {"foreground": "#FFFFFF"}},
        {"scope": ["support.type.property-name"], "settings": {"foreground": "#6FCF97"}},
        {"scope": ["meta.property-value"], "settings": {"foreground": "#D1CF00"}},
    
        {"scope": ["entity.other.attribute-name.pseudo-class"], "settings": {"foreground": "#68687B"}},
    
        {"scope": ["meta.function-call.generic"], "settings": {"fontStyle": "bold", "foreground": "#FFE400"}},
    
        {"scope": ["entity.other.inherited-class"], "settings": {"foreground": "#797993"}},
        {"scope": ["markup.tag"], "settings": {"foreground": "#D1CF00"}},
        {"scope": ["markup.attribute-name"], "settings": {"foreground": "#6FCF97"}},
    
        {"scope": ["markup.attribute-value"], "settings": {"foreground": "#B9BAE1"}},
    
        {"scope": ["markup.comment"], "settings": {"foreground": "#68687B"}},
    
        {"scope": ["entity.name.tag"], "settings": {"foreground": "#D1CF00"}},
        {
            "scope": ["storage.type"],
            "settings": {
                "foreground": "#6DBEE4"
            }
        },
        {
          "scope": ["keyword.control"],
          "settings": {
              "foreground": "#508BF9"
          }
      },
      {"scope": ["entity.name.section.markdown"], "settings": {"foreground": "#6DBEE4"}},
      {"scope": ["markup.bold.markdown", "markup.bold"], "settings": {"fontStyle": "bold", "foreground": "#F08080"}},
      {"scope": ["variable.language"], "settings": {"foreground": "#6DBEE4"}},
      {"scope": ["constant.language"], "settings": {"foreground": "#D1CF00"}}
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "parameter": "#D1CF00",
        "variable": "#B9BAE1",
        "property": "#B9BAE1",
        "type": "#6FCF97",
        "string": "#B9BAE1",
        "keyword": "#FFE400",
        "class": "#D1CF00",
        "function": "#6FCF97",
        "comment": {"fontStyle": "italic", "foreground": "#68687B"}
    }
}
