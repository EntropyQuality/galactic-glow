        "notifications.background": "#2C2C2C"
    },
    "tokenColors": [
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#A8D1FF"
            }
        },
        {
            "scope": ["entity.name.class", "entity.name.type", "entity.name.enum"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#E3A8FF"
            }
        },
        {
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#FFF8A8"
            }
        },
        {
            "scope": ["variable", "variable.parameter", "variable.other"],
            "settings": {
                "foreground": "#B6C3B6"
            }
        },
        {
            "scope": ["keyword.operator", "storage.modifier"],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": ["markup"],
            "settings": {
                "foreground": "#C7C7C7"
            }
        },
        {
            "scope": ["entity.name.function.magic"],
            "settings": {
                "foreground": "#FFF8A8"
            }
        },
        {
            "scope": ["storage.type.annotation", "storage.type"],
            "settings": {
                "foreground": "#C7C7C7"
            }
        },
        {
            "scope": ["string.quoted.docstring"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#A0A0A0"
            }
        },
        {
            "scope": ["support.type"],
            "settings": {
                "foreground": "#C7C7C7"
            }
        },
        {
            "scope": ["comment", "comment.block.documentation"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#808080"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#C3B6B6"
            }
        },
        {
            "scope": ["keyword.operator", "storage.modifier"],
            "settings": {
                "foreground": "#FFFFFF"
            }
        },
        {
            "scope": ["markup"],
            "settings": {
                "foreground": "#C3B6B6"
            }
        },
        {
            "scope": ["storage.type.annotation", "storage.type"],
            "settings": {
                "foreground": "#C3B6B6"
            }
        },
        {
            "scope": ["string.quoted.docstring"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#B6C3B6"
            }
        },
        {
            "scope": ["support.type"],
            "settings": {
                "foreground": "#E3A8FF"
            }
        },
        {
            "scope": ["meta.object-literal.key", "variable.other.property"],
            "settings": {
                "foreground": "#FF9F9F"
            }
        },
        {
            "scope": ["constant.language", "constant.character"],
            "settings": {
                "foreground": "#FFA8A8"
            }
        },
        {
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#FFD700"
            }
        },
        {
            "scope": ["entity.other.attribute-name"],
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "scope": ["markup.underline"],
            "settings": {
                "fontStyle": "underline",
                "foreground": "#FFD700"
            }
        },
        {
            "scope": ["markup.bold"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FFD700"
            }
        },
        {
            "scope": ["markup.italic"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#FFD700"
            }
        },
        {
            "scope": ["markup.inserted", "markup.deleted", "markup.changed"],
            "settings": {
                "foreground": "#FFC085"
            }
        },
        {
            "scope": ["invalid"],
            "settings": {
                "foreground": "#FF6B6B"
            }
        },
        {
            "scope": ["punctuation"],
            "settings": {
                "foreground": "#A64FA8"  
            }
        },
        {
            "scope": ["entity.name.section.markdown"],
            "settings": {
                "foreground": "#FFDAA5"  
            }
        },
        {
            "scope": ["markup.bold.markdown", "markup.bold"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FF9F9F"  
            }
        },
        {"scope": ["keyword.other.DML"], "settings": {"fontStyle": "bold", "foreground": "#A64FA8"}},   
        {"scope": ["keyword.function"], "settings": {"fontStyle": "bold", "foreground": "#FF9F9F"}},   
        {"scope": ["storage.modifier"], "settings": {"fontStyle": "bold", "foreground": "#FFDAA5"}},   
        {"scope": ["keyword.package"], "settings": {"fontStyle": "bold", "foreground": "#A8E3FF"}},   

    ],
    "semanticHighlighting": true
}