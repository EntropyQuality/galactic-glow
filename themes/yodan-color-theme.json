    "tokenColors": [
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#D1EDC1"
            }
        },
        {
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#BF8F6B"
            }
        },
        {
            "scope": ["entity.name.class", "entity.name.type", "entity.name.enum"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#A3BE8C"
            }
        },
        {
            "scope": ["entity.name.function"],
            "settings": {
                "foreground": "#C5D4C3"
            }
        },
        {
            "scope": ["variable", "variable.parameter", "variable.other"],
            "settings": {
                "foreground": "#8FB588"
            }
        },
        {
            "scope": ["keyword.operator", "storage.modifier"],
            "settings": {
                "foreground": "#D1EDC1"
            }
        },
        {
            "scope": ["comment"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5E715E"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#BF8F6B"
            }
        },
        {
            "scope": ["meta.object-literal.key", "variable.other.property"],
            "settings": {
                "foreground": "#D1EDC1"
            }
        },
        {
            "scope": ["support.type.property-name", "variable.other.readwrite"],
            "settings": {
                "foreground": "#D1EDC1"
            }
        },
        {
            "scope": ["entity.name.tag.html"],
            "settings": {
              "foreground": "#D1EDC1"
            }
          },
          {
            "scope": ["entity.other.attribute-name.html"],
            "settings": {
              "foreground": "#A3BE8C"
            }
          },
          {
            "scope": ["punctuation.definition.tag.html"],
            "settings": {
              "foreground": "#BF8F6B"
            }
          },
          {
            "scope": ["meta.tag.metadata.doctype.html"],
            "settings": {
              "foreground": "#8FB588"
            }
          },
          {
            "scope": ["comment.block.documentation.html"],
            "settings": {
              "fontStyle": "italic",
              "foreground": "#5E715E"
            }
          },
          {
            "scope": ["string.unquoted.html"],
            "settings": {
              "foreground": "#BF8F6B"
            }
          },
          {
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "#C5D4C3"
            }
        },
        {
            "scope": ["support.constant.property-value.css"],
            "settings": {
                "foreground": "#A3BE8C"
            }
        },
        {
            "scope": ["keyword.other.important.css"],
            "settings": {
                "foreground": "#D1EDC1"
            }
        },
        {
            "scope": ["entity.other.attribute-name.class.css"],
            "settings": {
                "foreground": "#8FB588"
            }
        },
        {
            "scope": ["entity.other.attribute-name.id.css"],
            "settings": {
                "foreground": "#BF8F6B"
            }
        },
        {
            "scope": ["comment.block.css"],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#5E715E"
            }
        },
        {
            "scope": ["meta.selector.css"],
            "settings": {
                "foreground": "#D9E17C"
            }
        },
        {
            "scope": ["entity.other.attribute-name.css"],
            "settings": {
                "foreground": "#36402D"
            }
        },
        {
            "scope": ["support.type.property-name.css"],
            "settings": {
                "foreground": "#7EA6C1"
            }
        },
        {
            "scope": ["storage.type.built-in"],
            "settings": {
                "foreground": "#D9E17C"
            }
        },
        {
            "scope": ["punctuation.definition.directive"],
            "settings": {
                "foreground": "#7EA6C1"
            }
        },
        {
            "scope": ["keyword.control.import.python"],
            "settings": {
                "foreground": "#7EA6C1"
            }
        },
        {
            "scope": ["entity.name.tag.html"],
            "settings": {
                "foreground": "#7EA6C1"
            }
        },
        {
            "scope": ["storage.type"],
            "settings": {
                "foreground": "#B6A2D2"
            }
        },
        {
            "scope": ["keyword.control.directive.include"],
            "settings": {
                "foreground": "#F6A5C0"
            }
        },
        {
            "scope": ["keyword.control"],
            "settings": {
                "foreground": "#75B1A9"
            }
        },
        {
            "scope": ["storage.type.function"],
            "settings": {
                "foreground": "#D9E17C"
            }
        },
        {
            "scope": ["entity.other.attribute-name.pseudo-class.css"],
            "settings": {
                "foreground": "#B6A2D2"
            }
        },
        {"scope": ["punctuation"], "settings": {"foreground": "#7EA6C1"}},
        {"scope": ["entity.name.section.markdown"], "settings": {"foreground": "#F6A5C0"}},
        {"scope": ["markup.bold.markdown", "markup.bold"], "settings": {"fontStyle": "bold", "foreground": "#D9E17C"}},
        {"scope": ["constant.language"], "settings": {"fontStyle": "italic", "foreground": "#FF69B4"}},
        {"scope": ["constant.numeric"], "settings": {"fontStyle": "italic", "foreground": "#ADD8E6"}},
        
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "property": {
            "foreground": "#C5D4C3"
        },
        "parameter": {
            "foreground": "#BF8F6B"
        },
        "variable": {
            "foreground": "#8FB588"
        },
        "type": {
            "foreground": "#A3BE8C"
        },
        "string": {
            "foreground": "#BF8F6B"
        },
        "keyword": {
            "foreground": "#D1EDC1"
        },
        "namespace": {
            "foreground": "#D1EDC1"
        }
    }
}
